{
  "name": "ponto-eletronico",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "ENVFILE=.env npx expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "mock-api": "json-server --watch ./mock-api/db.json --host 192.168.1.7 --port 5000",
    "localhost": "set EXPO_ENV=localhost && npx expo start -c",
    "dev": "set EXPO_ENV=dev && npx expo start --dev-client",
    "build:dev:client": "npx eas build --platform android --profile development",
    "prod": "set EXPO_ENV=.env.prod && npx expo start -c",
    "build:os:preview": "concurrently \"cd android\" \"gradlew assembleDebug --stacktrace\"",
    "build:os:dev": "concurrently \"npx expo prebuild\" \"cd android && gradlew assembleRelease --stacktrace\"",
    "build:os:prod": "concurrently \"npx expo prebuild\" \"cd android && gradlew assembleRelease --stacktrace\"",
    "build:ios:preview": "npx eas build --platform ios --profile preview",
    "build:ios:dev": "set EXPO_ENV=dev && npx eas build --platform ios --profile dev",
    "build:ios:prod": "set EXPO_ENV=prod && npx eas build --platform ios --profile prod"
  },
  "dependencies": {
    "@expo-google-fonts/roboto": "^0.2.3",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/stack": "^6.3.18",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "expo": "^49.0.16",
    "expo-build-properties": "~0.8.3",
    "expo-camera": "~13.4.4",
    "expo-constants": "~14.4.2",
    "expo-crypto": "~12.4.1",
    "expo-font": "~11.4.0",
    "expo-image-manipulator": "~11.3.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-local-authentication": "~13.4.1",
    "expo-location": "~16.1.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-sqlite": "~11.3.3",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "lottie-ios": "^3.5.0",
    "lottie-react-native": "^5.1.6",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-toast-message": "^2.1.6",
    "react-navigation": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/webpack-config": "^19.0.0",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.5",
    "@types/crypto-js": "^4.1.3",
    "@types/react": "~18.2.14",
    "@types/react-redux": "^7.1.30",
    "axios": "^1.6.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "formik": "^2.4.5",
    "json-server": "^0.17.4",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "private": true
}
