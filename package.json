{
  "name": "ponto-eletronico",
  "version": "1.0.0",
  "main": "./node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "mock-api": "json-server --watch ./mock-api/db.json --host 192.168.1.7 --port 5000",
    "localhost": "set EXPO_ENV=localhost && npx expo start -c",
    "dev": "set EXPO_ENV=dev && npx expo start --dev-client",
    "build:dev:client": "npx eas build --platform android --profile development",
    "prod": "set EXPO_ENV=.env.prod && npx expo start -c",
    "build:os:preview": "concurrently \"npx expo prebuild --no-install --platform android\" \"cd android  && gradlew clean  && gradlew assembleDebug --stacktrace\"",
    "build:os:dev": "concurrently \"npx expo prebuild --no-install --platform android\" \"cd android && gradlew clean && gradlew assembleRelease --stacktrace\"",
    "build:os:prod": "concurrently \"npx expo prebuild --no-install --platform android\" \"cd android && gradlew clean && gradlew assembleRelease --stacktrace\"",
    "build:ios:preview": "npx eas build --platform ios --profile preview",
    "build:ios:dev": "set EXPO_ENV=dev && npx eas build --platform ios --profile dev",
    "build:ios:prod": "set EXPO_ENV=prod && npx eas build --platform ios --profile prod"
  },
  "dependencies": {
